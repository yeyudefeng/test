select 
     id                             -- id
    ,name_prov                      -- 省
    ,code_prov                      -- 省code
    ,name_city                      -- 市
    ,code_city                      -- 市code
    ,name_coun                      -- 区
    ,code_coun                      -- 区code
    ,name_town                      -- 镇
    ,code_town                      -- 镇code
from guomiaozhishu.tb_township_area
